#------------SET CUSTOM NAME-------------#
execute if score @s PhysicRangedPurchased matches 1 in minecraft:overworld on passengers run data modify entity @s text set from block 700000 1 700000 front_text.messages[0]
execute unless score @s PhysicRangedPurchased matches 1 in minecraft:overworld run data modify entity @s CustomName set from block 700000 1 700000 front_text.messages[0]
#------------IF SCULK SLIME--------------#
execute if entity @s[tag=SculkSlime] run data modify entity @e[type=armor_stand,tag=SculkSlimeTexture,sort=nearest,limit=1] CustomName set from entity @s CustomName
