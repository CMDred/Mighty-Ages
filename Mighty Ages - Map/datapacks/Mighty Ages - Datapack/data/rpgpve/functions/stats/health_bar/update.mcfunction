#------------SOUND IF NEEDED-------------#
execute if entity @s[tag=VoidSentry] run function rpgpve:game/battle_wave/monsters/ai/void_sentry/damage_sound
#-----------------SETUP------------------#
tag @s add EditingMob
execute in minecraft:overworld run setblock 700000 1 700000 minecraft:air
#----------GET HEALTH PERCENTAGE---------#
scoreboard players operation #HealthPercentage Temp = @s Health
scoreboard players operation #HealthPercentage Temp *= #c100 Constant
scoreboard players operation #HealthPercentage Temp /= @s MaxHealth
execute if score @s Health matches ..-1 run scoreboard players set @s Health 0
